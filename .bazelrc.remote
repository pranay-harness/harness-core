startup --output_base=/tmp
build --javabase=@openjdk8u242_darwin_archive//:runtime
build --host_javabase=@openjdk8u242_darwin_archive//:runtime
build --java_toolchain=@bazel_tools//tools/jdk:toolchain_hostjdk8
