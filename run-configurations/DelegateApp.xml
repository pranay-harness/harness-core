<configuration name="DelegateApp" type="BlazeCommandRunConfigurationType" factoryName="Bazel Command" temporary="true">
    <blaze-settings handler-id="BlazeJavaRunConfigurationHandlerProvider" kind="java_binary"
                    debug_port="6006" blaze-command="run" keep-in-sync="false">
        <blaze-target>//260-delegate:module</blaze-target>
        <blaze-user-flag>
            --jvmopt="-Xbootclasspath/p:${HOME}/.m2/repository/org/mortbay/jetty/alpn/alpn-boot/8.1.13.v20181017/alpn-boot-8.1.13.v20181017.jar"
        </blaze-user-flag>
        <blaze-user-exe-flag>$PROJECT_DIR$/../260-delegate/config-delegate.yml</blaze-user-exe-flag>
    </blaze-settings>
    <method v="2">
        <option name="Blaze.BeforeRunTask" enabled="true"/>
    </method>
</configuration>