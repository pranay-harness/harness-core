{
  "allOf": [
    {
      "$ref": "#/definitions/ArtifactoryAuthCredentials"
    },
    {
      "type": "object",
      "required": [
        "passwordRef",
        "username"
      ],
      "properties": {
        "passwordRef": {
          "type": "string",
          "pattern": "^(account.)?(org.)?(\\.)?[a-zA-Z0-9_$]*$"
        },
        "username": {
          "type": "string"
        }
      }
    }
  ],
  "$schema": "http://json-schema.org/draft-07/schema#"
}