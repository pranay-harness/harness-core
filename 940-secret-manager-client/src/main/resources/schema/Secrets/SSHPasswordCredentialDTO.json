{
  "allOf": [
    {
      "$ref": "#/definitions/SSHCredentialSpecDTO"
    },
    {
      "type": "object",
      "required": [
        "password",
        "userName"
      ],
      "properties": {
        "password": {
          "type": "string",
          "pattern": "^(account.)?(org.)?(\\.)?[a-zA-Z0-9_$]*$"
        },
        "userName": {
          "type": "string"
        }
      }
    }
  ],
  "$schema": "http://json-schema.org/draft-07/schema#"
}